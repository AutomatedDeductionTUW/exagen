(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (h (g c x)))) (not (= (f b) (f (g y y)))))))
(assert (or (not (P (f a))) (not (= (f b) (f (g d d))))))
(assert (not (or (P (h (g c (h b)))) (not (P (f a))) (not (= (f b) (f (g d d)))))))
(check-sat)
