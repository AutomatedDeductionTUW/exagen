(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (f (g (h b) a)))) (not (P (g d (g a a)))) (= (h d) (f (f d)))))
(assert (forall ((x A) (y A)) (or (P (f (g y a))) (= (h x) (f (f x))))))
(assert (not (or (not (P (g d (g a a)))) (= (h d) (f (f d))))))
(check-sat)
