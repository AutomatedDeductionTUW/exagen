(declare-sort A 0)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (P (f (h x))) (not (= (g d y) (h (h y)))))))
(assert (or (not (P (g (h d) d))) (not (= (g d d) (h (h d))))))
(assert (not (or (not (P (f (h (f c))))) (not (P (g (h d) d))) (not (= (g d d) (h (h d)))))))
(check-sat)
