(declare-sort A 0)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (h y))) (= (g (g x x) x) (g d (h b))))))
(assert (or (not (P (g (g d c) b))) (= (g (g d d) d) (g d (h b)))))
(assert (not (or (P (h (h b))) (not (P (g (g d c) b))) (= (g (g d d) d) (g d (h b))))))
(check-sat)
