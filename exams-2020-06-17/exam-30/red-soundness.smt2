(declare-sort A 0)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (P (h (h (f b)))) (not (P (h d))) (= (g b (g b c)) (g d d))))
(assert (forall ((x A) (y A)) (or (not (P (h (h y)))) (= (g b (g b c)) (g x x)))))
(assert (not (or (not (P (h d))) (= (g b (g b c)) (g d d)))))
(check-sat)
