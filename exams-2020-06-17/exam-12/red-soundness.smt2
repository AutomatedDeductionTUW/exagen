(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (f (h (g c c))))) (P (h (h a))) (= (f (g c c)) (h b))))
(assert (forall ((x A) (y A)) (or (P (f (h y))) (= (f (g x x)) (h b)))))
(assert (not (or (P (h (h a))) (= (f (g c c)) (h b)))))
(check-sat)
