(declare-sort A 0)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (P (f (f c))) (not (P (h c))) (= (g (h c) (h d)) (g (g c c) (f c)))))
(assert (forall ((x A) (y A)) (or (not (P (f y))) (= (g (h x) (h d)) (g (g c c) (f x))))))
(assert (not (or (not (P (h c))) (= (g (h c) (h d)) (g (g c c) (f c))))))
(check-sat)
