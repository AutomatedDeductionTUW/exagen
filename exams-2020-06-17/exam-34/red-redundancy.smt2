(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (g (f a) (f x)))) (= (g d y) (g y d)))))
(assert (or (not (P (g (h a) (g a a)))) (= (g d a) (g a d))))
(assert (not (or (P (g (f a) (f (f b)))) (not (P (g (h a) (g a a)))) (= (g d a) (g a d)))))
(check-sat)
