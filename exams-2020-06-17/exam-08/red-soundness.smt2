(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (P (h (f (h a)))) (P (g (h d) b)) (not (= (f a) (g (g b d) (g b b))))))
(assert (forall ((x A) (y A)) (or (not (P (h (f y)))) (not (= (f a) (g (g x d) (g b x)))))))
(assert (not (or (P (g (h d) b)) (not (= (f a) (g (g b d) (g b b)))))))
(check-sat)
