(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (h (g x b)))) (not (= (f d) (h (g y y)))))))
(assert (or (not (P (h (f d)))) (not (= (f d) (h (g a a))))))
(assert (not (or (P (h (g (g d d) b))) (not (P (h (f d)))) (not (= (f d) (h (g a a)))))))
(check-sat)
