(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (g (g b a) c))) (not (P (h (h d)))) (not (= (h a) (h (f a))))))
(assert (forall ((x A) (y A)) (or (P (g y c)) (not (= (h x) (h (f x)))))))
(assert (not (or (not (P (h (h d)))) (not (= (h a) (h (f a)))))))
(check-sat)
