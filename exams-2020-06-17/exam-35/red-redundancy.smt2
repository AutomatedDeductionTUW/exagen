(declare-sort A 0)
(declare-const a A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (P (f (f x))) (= (g y y) (f (g y y))))))
(assert (or (not (P (g a (h c)))) (= (g a a) (f (g a a)))))
(assert (not (or (not (P (f (f (h d))))) (not (P (g a (h c)))) (= (g a a) (f (g a a))))))
(check-sat)
