(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (h x))) (not (= (f (g y c)) (g y c))))))
(assert (or (P (f (h b))) (not (= (f (g b c)) (g b c)))))
(assert (not (or (P (h (g a b))) (P (f (h b))) (not (= (f (g b c)) (g b c))))))
(check-sat)
