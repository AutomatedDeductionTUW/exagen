(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (f (g a x)))) (= (g (h y) (f b)) (g y d)))))
(assert (or (P (g (h c) (h d))) (= (g (h a) (f b)) (g a d))))
(assert (not (or (P (f (g a (h b)))) (P (g (h c) (h d))) (= (g (h a) (f b)) (g a d)))))
(check-sat)
