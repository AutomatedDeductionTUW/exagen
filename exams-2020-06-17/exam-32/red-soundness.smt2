(declare-sort A 0)
(declare-const a A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (g d (h (f d))))) (not (P (g c (g c c)))) (= (g (g a a) a) (h (h a)))))
(assert (forall ((x A) (y A)) (or (P (g d (h x))) (= (g (g y y) y) (h (h y))))))
(assert (not (or (not (P (g c (g c c)))) (= (g (g a a) a) (h (h a))))))
(check-sat)
