(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (f (g a x)))) (not (= (h y) (h y))))))
(assert (or (P (g (h b) (h a))) (not (= (h c) (h c)))))
(assert (not (or (P (f (g a (f c)))) (P (g (h b) (h a))) (not (= (h c) (h c))))))
(check-sat)
