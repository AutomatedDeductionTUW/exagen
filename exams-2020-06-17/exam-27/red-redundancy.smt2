(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (not (P (f x))) (not (= (f b) (h (g y y)))))))
(assert (or (P (g a c)) (not (= (f b) (h (g d d))))))
(assert (not (or (P (f (f c))) (P (g a c)) (not (= (f b) (h (g d d)))))))
(check-sat)
