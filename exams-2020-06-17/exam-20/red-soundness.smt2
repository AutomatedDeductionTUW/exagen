(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (P (f (g (f d) d))) (P (h (f a))) (not (= (f b) (g (h d) b)))))
(assert (forall ((x A) (y A)) (or (not (P (f (g x d)))) (not (= (f y) (g (h d) y))))))
(assert (not (or (P (h (f a))) (not (= (f b) (g (h d) b))))))
(check-sat)
