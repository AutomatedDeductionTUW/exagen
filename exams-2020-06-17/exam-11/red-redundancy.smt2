(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (P (h y)) (not (= (g x x) (g (g x x) b))))))
(assert (or (P (g (h c) (h d))) (not (= (g a a) (g (g a a) b)))))
(assert (not (or (not (P (h (h c)))) (P (g (h c) (h d))) (not (= (g a a) (g (g a a) b))))))
(check-sat)
