(declare-sort A 0)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (h (f c)))) (P (f (f c))) (= (g (g d c) (f d)) (g c c))))
(assert (forall ((x A) (y A)) (or (P (h y)) (= (g (g d c) (f d)) (g x x)))))
(assert (not (or (P (f (f c))) (= (g (g d c) (f d)) (g c c)))))
(check-sat)
