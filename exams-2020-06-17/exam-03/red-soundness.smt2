(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (f (h a)))) (not (P (f (h d)))) (= (g c (g c b)) (g (h c) (f c)))))
(assert (forall ((x A) (y A)) (or (P (f x)) (= (g c (g c b)) (g (h y) (f y))))))
(assert (not (or (not (P (f (h d)))) (= (g c (g c b)) (g (h c) (f c))))))
(check-sat)
