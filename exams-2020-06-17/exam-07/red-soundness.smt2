(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (P (g c (f (g b d)))) (P (h (h a))) (= (h (f c)) (f c))))
(assert (forall ((x A) (y A)) (or (not (P (g c (f y)))) (= (h (f x)) (f x)))))
(assert (not (or (P (h (h a))) (= (h (f c)) (f c)))))
(check-sat)
