(declare-sort A 0)
(declare-const b A)
(declare-const c A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (g (g c b) c))) (P (g (f c) (h c))) (not (= (h (g b c)) (h (g b b))))))
(assert (forall ((x A) (y A)) (or (P (g x c)) (not (= (h (g y c)) (h (g y y)))))))
(assert (not (or (P (g (f c) (h c))) (not (= (h (g b c)) (h (g b b)))))))
(check-sat)
