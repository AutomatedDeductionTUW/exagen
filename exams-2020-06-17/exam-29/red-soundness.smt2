(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (or (not (P (f (g (g c d) c)))) (P (g d (f d))) (= (h b) (g b (g a a)))))
(assert (forall ((x A) (y A)) (or (P (f (g x c))) (= (h b) (g b (g y y))))))
(assert (not (or (P (g d (f d))) (= (h b) (g b (g a a))))))
(check-sat)
