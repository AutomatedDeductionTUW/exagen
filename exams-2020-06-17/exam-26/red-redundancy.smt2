(declare-sort A 0)
(declare-const a A)
(declare-const b A)
(declare-const c A)
(declare-const d A)
(declare-fun f (A) A)
(declare-fun g (A A) A)
(declare-fun h (A) A)
(declare-fun P (A) Bool)
(assert (forall ((x A) (y A)) (or (P (g y (g a c))) (not (= (g x c) (f x))))))
(assert (or (not (P (g (h a) (g b b)))) (not (= (g d c) (f d)))))
(assert (not (or (not (P (g (h b) (g a c)))) (not (P (g (h a) (g b b)))) (not (= (g d c) (f d))))))
(check-sat)
